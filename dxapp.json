{
    "name": "eggd_staraligner",
    "title": "eggd_staraligner",
    "summary": "Sentieon STAR aligner",
    "dxapi": "1.0.0",
    "version": "1.0.0",
    "properties": {
      "githubRelease": "v1.0.0"
    },
    "inputSpec": [
        {
        "name": "left_fq",
        "label": "left_fq",
        "help": "gzipped fastq for R1",
        "class": "file",
        "patterns": ["*.fastq.gz"],
        "optional": true
        },
        {
        "name": "right_fq",
        "label": "right_fq",
        "help": "gzipped fastq for R2",
        "class": "file",
        "patterns": ["*.fastq.gz"],
        "optional": true
        },
        {
        "name": "sentieon_tar",
        "label": "sentieon_tar",
        "help": "tarballed sentieon",
        "class": "file",
        "patterns": ["sentieon-genomics-*.tar.gz"],
        "optional": false
        }
    ],
    "outputSpec": [
      {
        "name": "output",
        "label": "Output",
        "class": "file",
        "optional": true,
        "patterns": [
          "*"
        ],
        "help": ""
      }
    ],
    "runSpec": {
      "distribution": "Ubuntu",
      "release": "20.04",
      "version": "0",
      "interpreter": "bash",
      "file": "src/script.sh",
      "timeoutPolicy": {
        "*": {
          "hours": 1
        }
      }
    },
    "developers":[
      "org-emee_1"
    ],
    "authorizedUsers": [
      "org-emee_1"
    ],
    "access": {
      "project": "CONTRIBUTE",
      "network": ["*"],
      "allProjects": "VIEW"
    },
    "regionalOptions": {
      "aws:eu-central-1": {
        "systemRequirements": {
          "*": {
            "instanceType": "mem1_ssd1_v2_x4"
          }
        }
      }
    }
  }